{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase":"error",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    }
}
